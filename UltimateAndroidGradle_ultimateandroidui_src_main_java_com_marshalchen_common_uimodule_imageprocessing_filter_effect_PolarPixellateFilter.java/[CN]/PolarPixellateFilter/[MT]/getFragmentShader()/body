{
  return "precision mediump float;\n" + "uniform sampler2D " + UNIFORM_TEXTURE0 + ";\n"+ "varying vec2 "+ VARYING_TEXCOORD+ ";\n"+ "uniform vec2 "+ UNIFORM_FRACTIONAL_SIZE+ ";\n"+ "uniform vec2 "+ UNIFORM_CENTER+ ";\n"+ "void main(){\n"+ "  highp vec2 normCoord = 2.0 * "+ VARYING_TEXCOORD+ " - 1.0;\n"+ "  highp vec2 normCenter = 2.0 * "+ UNIFORM_CENTER+ " - 1.0;\n"+ "  normCoord -= normCenter;\n"+ "  highp float r = length(normCoord);\n"+ "  highp float phi = atan(normCoord.y, normCoord.x);\n"+ "  r = r - mod(r, "+ UNIFORM_FRACTIONAL_SIZE+ ".x) + 0.03;\n"+ "  phi = phi - mod(phi, "+ UNIFORM_FRACTIONAL_SIZE+ ".y);\n"+ "  normCoord.x = r * cos(phi);\n"+ "  normCoord.y = r * sin(phi);\n"+ "  normCoord += normCenter;\n"+ "  mediump vec2 textureCoordinateToUse = normCoord / 2.0 + 0.5;\n"+ "  gl_FragColor = texture2D("+ UNIFORM_TEXTURE0+ ", textureCoordinateToUse);\n"+ "}\n";
}
