{
  boolean flag=true;
  if (!(string != null && string.trim().length() > 0 && !string.equals("null") && !string.trim().equals("")))   return false;
  for (  String s : strings) {
    if (s == null || string.trim().length() == 0 || s.equals("null")) {
      flag=false;
      break;
    }
  }
  return flag;
}
