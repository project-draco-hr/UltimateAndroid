{
  View v=freeflowItem.view;
  if (v == null) {
    return null;
  }
  Rect of=new Rect();
  of.left=(int)(v.getLeft() + v.getTranslationX());
  of.top=(int)(v.getTop() + v.getTranslationY());
  of.right=(int)(v.getRight() + v.getTranslationX());
  of.bottom=(int)(v.getBottom() + v.getTranslationY());
  return of;
}
