{
  getAnimatorSet().playTogether(ObjectAnimator.ofFloat(view,"rotation",1080,720,360,0).setDuration(DURATION),ObjectAnimator.ofFloat(view,"alpha",0,1).setDuration(DURATION * 3 / 2),ObjectAnimator.ofFloat(view,"scaleX",0.1f,0.5f,1).setDuration(DURATION),ObjectAnimator.ofFloat(view,"scaleY",0.1f,0.5f,1).setDuration(DURATION));
}
