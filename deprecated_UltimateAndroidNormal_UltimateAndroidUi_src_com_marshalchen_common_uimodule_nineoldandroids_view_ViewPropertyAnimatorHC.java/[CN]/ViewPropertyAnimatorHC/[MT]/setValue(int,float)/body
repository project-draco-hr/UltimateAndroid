{
  View v=mView.get();
  if (v != null) {
switch (propertyConstant) {
case TRANSLATION_X:
      v.setTranslationX(value);
    break;
case TRANSLATION_Y:
  v.setTranslationY(value);
break;
case ROTATION:
v.setRotation(value);
break;
case ROTATION_X:
v.setRotationX(value);
break;
case ROTATION_Y:
v.setRotationY(value);
break;
case SCALE_X:
v.setScaleX(value);
break;
case SCALE_Y:
v.setScaleY(value);
break;
case X:
v.setX(value);
break;
case Y:
v.setY(value);
break;
case ALPHA:
v.setAlpha(value);
break;
}
}
}
