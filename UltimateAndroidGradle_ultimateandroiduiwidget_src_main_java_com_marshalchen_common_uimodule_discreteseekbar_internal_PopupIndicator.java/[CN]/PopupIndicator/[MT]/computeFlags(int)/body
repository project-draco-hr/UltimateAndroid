{
  curFlags&=~(WindowManager.LayoutParams.FLAG_IGNORE_CHEEK_PRESSES | WindowManager.LayoutParams.FLAG_NOT_FOCUSABLE | WindowManager.LayoutParams.FLAG_NOT_TOUCHABLE| WindowManager.LayoutParams.FLAG_WATCH_OUTSIDE_TOUCH| WindowManager.LayoutParams.FLAG_LAYOUT_NO_LIMITS| WindowManager.LayoutParams.FLAG_ALT_FOCUSABLE_IM);
  curFlags|=WindowManager.LayoutParams.FLAG_IGNORE_CHEEK_PRESSES;
  curFlags|=WindowManager.LayoutParams.FLAG_NOT_FOCUSABLE;
  curFlags|=WindowManager.LayoutParams.FLAG_NOT_TOUCHABLE;
  curFlags|=WindowManager.LayoutParams.FLAG_LAYOUT_NO_LIMITS;
  return curFlags;
}
