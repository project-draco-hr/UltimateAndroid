{
switch (touchMode) {
case (FreeFlowContainer.TOUCH_MODE_REST):
    return "touch_mode_rest";
case (FreeFlowContainer.TOUCH_MODE_DOWN):
  return "touch_mode_down";
case (FreeFlowContainer.TOUCH_MODE_TAP):
return "touch_mode_tap";
case (FreeFlowContainer.TOUCH_MODE_SCROLL):
return "touch_mode_scroll";
case (FreeFlowContainer.TOUCH_MODE_FLING):
return "touch_mode_fling";
case (FreeFlowContainer.TOUCH_MODE_DONE_WAITING):
return "touch_mode_done_waiting";
}
return "unknown touch event: " + touchMode;
}
