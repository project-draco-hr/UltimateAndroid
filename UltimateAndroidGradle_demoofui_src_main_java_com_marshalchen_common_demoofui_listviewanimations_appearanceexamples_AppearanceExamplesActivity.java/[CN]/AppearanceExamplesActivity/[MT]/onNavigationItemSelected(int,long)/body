{
switch (itemPosition) {
case 0:
    setAlphaAdapter();
  return true;
case 1:
setLeftAdapter();
return true;
case 2:
setRightAdapter();
return true;
case 3:
setBottomAdapter();
return true;
case 4:
setBottomRightAdapter();
return true;
case 5:
setScaleAdapter();
return true;
default :
return false;
}
}
