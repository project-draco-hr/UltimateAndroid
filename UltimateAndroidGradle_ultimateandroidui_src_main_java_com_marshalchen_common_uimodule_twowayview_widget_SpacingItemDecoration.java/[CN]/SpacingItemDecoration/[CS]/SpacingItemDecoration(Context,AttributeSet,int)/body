{
  final TypedArray a=context.obtainStyledAttributes(attrs,R.styleable.twowayview_SpacingItemDecoration,defStyle,0);
  final int verticalSpacing=Math.max(0,a.getInt(R.styleable.twowayview_SpacingItemDecoration_android_verticalSpacing,0));
  final int horizontalSpacing=Math.max(0,a.getInt(R.styleable.twowayview_SpacingItemDecoration_android_horizontalSpacing,0));
  a.recycle();
  mItemSpacing=new ItemSpacingOffsets(verticalSpacing,horizontalSpacing);
}
