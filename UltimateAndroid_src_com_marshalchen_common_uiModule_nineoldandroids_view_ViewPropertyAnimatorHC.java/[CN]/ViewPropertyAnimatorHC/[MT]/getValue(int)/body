{
  View v=mView.get();
  if (v != null) {
switch (propertyConstant) {
case TRANSLATION_X:
      return v.getTranslationX();
case TRANSLATION_Y:
    return v.getTranslationY();
case ROTATION:
  return v.getRotation();
case ROTATION_X:
return v.getRotationX();
case ROTATION_Y:
return v.getRotationY();
case SCALE_X:
return v.getScaleX();
case SCALE_Y:
return v.getScaleY();
case X:
return v.getX();
case Y:
return v.getY();
case ALPHA:
return v.getAlpha();
}
}
return 0;
}
