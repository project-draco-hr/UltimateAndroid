{
  if (keyData == null) {
    return null;
  }
  int expectedStringLen=keyData.length * 2;
  StringBuilder sb=new StringBuilder(expectedStringLen);
  for (int i=0; i < keyData.length; i++) {
    String hexStr=Integer.toString(keyData[i] & 0x00FF,16);
    if (hexStr.length() == 1) {
      hexStr="0" + hexStr;
    }
    sb.append(hexStr);
  }
  return sb.toString();
}
