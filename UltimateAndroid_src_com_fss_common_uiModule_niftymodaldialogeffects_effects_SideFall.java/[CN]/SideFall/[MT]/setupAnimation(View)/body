{
  getAnimatorSet().playTogether(ObjectAnimator.ofFloat(view,"scaleX",2,1.5f,1).setDuration(DURATION),ObjectAnimator.ofFloat(view,"scaleY",2,1.5f,1).setDuration(DURATION),ObjectAnimator.ofFloat(view,"rotation",25,0).setDuration(DURATION),ObjectAnimator.ofFloat(view,"translationX",80,0).setDuration(DURATION),ObjectAnimator.ofFloat(view,"alpha",0,1).setDuration(DURATION * 3 / 2));
}
